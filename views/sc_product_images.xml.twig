{% filter spaceless %}
<?xml version="1.0" encoding="UTF-8" ?>
<Request>
    {% for product in products %}
        <ProductImage>
            {# @var product \SellerCenter\Model\Product #}
            <SellerSku>{{ product.sellerSku }}</SellerSku>
            <Images>
                {% for image in product.images|slice(0,8) %}
                    <Image>{{ image }}</Image>
                {% endfor %}
            </Images>
        </ProductImage>
    {% endfor %}
</Request>
{% endfilter %}